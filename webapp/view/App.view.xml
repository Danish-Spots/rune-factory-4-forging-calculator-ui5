    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true">
    <Shell>
        <App>
            <pages>
                <Page title="{i18n>title}">
                    <content>
                        <mvc:XMLView viewName="rf.calculator.view.HelloPanel"></mvc:XMLView>
