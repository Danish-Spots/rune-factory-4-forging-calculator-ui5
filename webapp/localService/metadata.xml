<edmx:Edmx Version="1.0" xmlns:edmx="http://schemas.openxmlformats.org/office/spreadsheetml/2006/relationships" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:sap="http://www.sap.com/odata" xmlns:atom="http://www.w3.org/2005/Atom">
  <edmx:DataServices>
    <Schema xmlns="http://schemas.microsoft.com/ado/2007/06/edm" Namespace="MaterialsModel">
      
      <!-- Entity Types -->
      <EntityType Name="Material">
        <Key>
          <PropertyRef Name="Id"/>
        </Key>
        <Property Name="Id" Type="Edm.Int32" Nullable="false"/>
        <Property Name="Name" Type="Edm.String" Nullable="false"/>
        <Property Name="Rarity" Type="Edm.Int32" Nullable="false"/>
        <Property Name="Category" Type="Edm.String" Nullable="false"/>
        <Property Name="Difficulty" Type="Edm.Int32" Nullable="false"/>
        <Property Name="StatInfo" Type="Edm.String" Nullable="true"/>
      </EntityType>

      <EntityType Name="Location">
        <Key>
          <PropertyRef Name="Id"/>
        </Key>
        <Property Name="Id" Type="Edm.Int32" Nullable="false"/>
        <Property Name="Name" Type="Edm.String" Nullable="false"/>
      </EntityType>

      <EntityType Name="Monster">
        <Key>
          <PropertyRef Name="Id"/>
        </Key>
        <Property Name="Id" Type="Edm.Int32" Nullable="false"/>
        <Property Name="Name" Type="Edm.String" Nullable="false"/>
        <Property Name="Condition" Type="Edm.String" Nullable="true"/>
      </EntityType>

      <EntityType Name="Fishing">
        <Key>
          <PropertyRef Name="Id"/>
        </Key>
        <Property Name="Id" Type="Edm.Int32" Nullable="false"/>
        <Property Name="Location" Type="Edm.String" Nullable="false"/>
        <Property Name="Area" Type="Edm.String" Nullable="false"/>
      </EntityType>

      <EntityType Name="Chopping">
        <Key>
          <PropertyRef Name="Id"/>
        </Key>
        <Property Name="Id" Type="Edm.Int32" Nullable="false"/>
        <Property Name="Name" Type="Edm.String" Nullable="false"/>
      </EntityType>

      <EntityType Name="Other">
        <Key>
          <PropertyRef Name="Id"/>
        </Key>
        <Property Name="Id" Type="Edm.Int32" Nullable="false"/>
        <Property Name="Name" Type="Edm.String" Nullable="false"/>
        <Property Name="Locations" Type="Edm.String" Nullable="false"/>
      </EntityType>

      <!-- Navigation Properties -->
      <EntityType Name="Drop">
        <Key>
          <PropertyRef Name="Id"/>
        </Key>
        <Property Name="Id" Type="Edm.Int32" Nullable="false"/>
        <NavigationProperty Name="Locations" Relationship="MaterialsModel.Drops_Locations" ToRole="Location" FromRole="Drop"/>
        <NavigationProperty Name="Monsters" Relationship="MaterialsModel.Drops_Monsters" ToRole="Monster" FromRole="Drop"/>
        <NavigationProperty Name="Fishing" Relationship="MaterialsModel.Drops_Fishing" ToRole="Fishing" FromRole="Drop"/>
        <NavigationProperty Name="Chopping" Relationship="MaterialsModel.Drops_Chopping" ToRole="Chopping" FromRole="Drop"/>
        <NavigationProperty Name="Other" Relationship="MaterialsModel.Drops_Other" ToRole="Other" FromRole="Drop"/>
      </EntityType>

      <!-- Association Sets (Many-to-Many Relationships) -->
      <Association Name="Drops_Locations">
        <End Role="Drop" Type="MaterialsModel.Drop" Multiplicity="1"/>
        <End Role="Location" Type="MaterialsModel.Location" Multiplicity="*" />
      </Association>

      <Association Name="Drops_Monsters">
        <End Role="Drop" Type="MaterialsModel.Drop" Multiplicity="1"/>
        <End Role="Monster" Type="MaterialsModel.Monster" Multiplicity="*" />
      </Association>

      <Association Name="Drops_Fishing">
        <End Role="Drop" Type="MaterialsModel.Drop" Multiplicity="1"/>
        <End Role="Fishing" Type="MaterialsModel.Fishing" Multiplicity="*" />
      </Association>

      <Association Name="Drops_Chopping">
        <End Role="Drop" Type="MaterialsModel.Drop" Multiplicity="1"/>
        <End Role="Chopping" Type="MaterialsModel.Chopping" Multiplicity="*" />
      </Association>

      <Association Name="Drops_Other">
        <End Role="Drop" Type="MaterialsModel.Drop" Multiplicity="1"/>
        <End Role="Other" Type="MaterialsModel.Other" Multiplicity="*" />
      </Association>

      <!-- Entity Sets -->
      <EntitySet Name="Materials" EntityType="MaterialsModel.Material"/>
      <EntitySet Name="Locations" EntityType="MaterialsModel.Location"/>
      <EntitySet Name="Monsters" EntityType="MaterialsModel.Monster"/>
      <EntitySet Name="Fishing" EntityType="MaterialsModel.Fishing"/>
      <EntitySet Name="Chopping" EntityType="MaterialsModel.Chopping"/>
      <EntitySet Name="Other" EntityType="MaterialsModel.Other"/>
      <EntitySet Name="Drops" EntityType="MaterialsModel.Drop"/>

    </Schema>
  </edmx:DataServices>
</edmx:Edmx>
