{
	"compilerOptions": {
		"target": "es2023",
		"module": "es2022",
		"moduleResolution": "node",
		"skipLibCheck": true,
		"allowJs": true,
		"strict": true,
		"strictPropertyInitialization": false,
		"rootDir": "webapp",
		"baseUrl": "./",
		"exactOptionalPropertyTypes": true,
		"paths": {
			"rf/calculator/*": ["webapp/*"]
		}
	},
	"include": ["webapp/**/*"]
}
